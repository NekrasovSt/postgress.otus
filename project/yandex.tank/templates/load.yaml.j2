overload:
  enabled: true
  package: yandextank.plugins.DataUploader
  token_file: "token.txt"
phantom:
  address: {{ hostvars['dbproxy'].ip }}:8080
  ammofile: ammo.txt
  header_http: "1.1"
  ammo_type: uri
  load_profile:
    load_type: rps
    schedule: line(5, 30, 1m) step(30,90,5, 1m)
  instances: 10
  phantom_http_entity: 30M
  ssl: false
autostop:
  autostop:
    - http(5xx,10%,30s)
    - http(4xx,10%,5s)
console:
  enabled: true
telegraf:
  enabled: false